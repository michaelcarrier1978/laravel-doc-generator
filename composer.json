{
    "name": "laravel-docs/generator",
    "description": "AI-powered Laravel controller documentation generator with Confluence integration",
    "keywords": ["laravel", "documentation", "claude", "ai", "confluence", "generator"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LaravelDocs\\Generator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Laravel Docs Team",
            "email": "support@laravel-doc.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/console": "^6.0|^7.0",
        "nikic/php-parser": "^4.15|^5.0",
        "guzzlehttp/guzzle": "^7.5",
        "vlucas/phpdotenv": "^5.5",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelDocs\\Generator\\LaravelDocGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
